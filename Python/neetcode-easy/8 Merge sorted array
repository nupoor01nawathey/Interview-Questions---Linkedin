import ListNode

def merge_sorted_lists(list1, list2):
    dummy = curr = ListNode()
            
    while list1 and list2:
        if list1.val < list2.val:
            curr.next = list1
            list1 = list1.next
        else:
            curr.next = list2
            list2 = list2.next
        curr = curr.next
                
        curr.next = list1 or list2
                
    return dummy.next


print(merge_sorted_lists([], []))
print(merge_sorted_lists([], [0]))
print(merge_sorted_lists([1,2,4], [1,3,4]))


# 
# Time complexity 
# Space complexity 
